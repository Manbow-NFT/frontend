    "@thirdweb-dev/sdk": "^3",
    "ethers": "^5.7.2",
    "framer-motion": "^9.0.1",
    "next": "^13",
    "react": "^18.2",
    "react-dom": "^18.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.11",
    "@types/react": "^18",
    "eslint": "^8.29.0",
    "eslint-config-next": "^13",
    "eslint-config-prettier": "^8.6.0",
    "prettier": "^2.8.3",
    "typescript": "^4.9.4"
  }
